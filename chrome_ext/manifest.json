{
  "name": "Phone Octo Ninja",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "permissions": [
    "http://api.flickr.com/",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["googleapi.js", "jquery.js", "bridge.min.js", "binary.js", "binaryremote.js", "test.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
